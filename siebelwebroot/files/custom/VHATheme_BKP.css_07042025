.VHAmodalDialogAlert {
    height: 100% !important;
    left: 0 !important;
    top: 0 !important;
    width: 100% !important;
    z-index: 100002;
    position: fixed;
    -webkit-transition: opacity 400ms ease-in;
    -moz-transition: opacity 400ms ease-in;
    transition: opacity 400ms ease-in;
    background: rgba(0, 0, 0, 0.7);
    overflow-y: auto;
}

.VHAmodalDialogAlert>div {
    padding: 20px 30px 20px 40px;
    margin-top: 650px;
    width: 500px;
    position: relative;
    margin: 10% auto;
    background: #fff;
    background: -moz-linear-gradient(#fff, #FFF);
    background: -webkit-linear-gradient(#fff, #FFF);
    background: -o-linear-gradient(#fff, #FFF);
    border-radius: 5px;
    border: 2px solid #cccccc;
    height: 200px;
}

.VHADialogClose {
    background: #FFFFFF;
    color: #000000;
    line-height: 25px;
    position: absolute;
    right: 50px;
    text-align: center;
    top: 25px;
    width: 24px;
    text-decoration: none;
    font-weight: bold;
    -webkit-border-radius: 12px;
    -moz-border-radius: 12px;
    border-radius: 12px;
    font-size: x-large;
    cursor: pointer;
}

.modalDialog_C .VHADialogClose {
    left: 1500px !important;
}

.VHADialogClose>img {
    height: 20px;
}

